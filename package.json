  "scripts": {
    "patch": "sed -i -e 's/if(c\\.initial) return;/if(!c||c.initial) return;/g' ./node_modules/extract-text-webpack-plugin/index.js",
    "build": "gatsby build",
    "clean": "rm -rf ./.cache && rm -rf ./node_modules && rm -rf ./public && rm -f ./yarn.lock",
    "clear": "rm -rf ./.cache && rm -rf ./public",
    "deploy": "s3-deploy './public/**' --cwd './public/' --region 'eu-central-1' --bucket 'trafficdesign.pl' --distId CF_DIST_ID --invalidate '/INV_PATH_1 /INV_PATH_2'",
    "start": "./node_modules/gatsby-cli/lib/index.js develop",
    "bundle": "./node_modules/gatsby-cli/lib/index.js build",
    "develop": "gatsby develop"
  }
